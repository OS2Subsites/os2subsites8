# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore sensitive information
/web/sites/*/settings.php
/web/sites/*/settings.local.php
/web/sites/sites.php

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore config files. Accordingly to bussines requirements we should not store
# configuration files in git.
/config/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Including libraries that are not managed via composer
!web/libraries
web/libraries/*
!web/libraries/fakeobjects
!web/libraries/iframe
!web/libraries/a11ychecker
!web/libraries/balloonpanel

# Ignore custom files generated by composer
/scripts/os2subsites*

# Docker settings
/.docker

# Env sensitive data
/tmp
/logs
/public_html

# Translation are covered by custom tranlsation file.
# If you still need translation to be added into git. Use -f flag
/translations/*
!/translations/.gitkeep

# Ignore files generated by PhpStorm
/.idea/

# Ignore .env files as they are personal
/.env
.editorconfig
.gitattributes

.DS_Store
*.log
*.sql
*.sql.gz
